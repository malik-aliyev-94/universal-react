{
  "name": "universal-react",
  "version": "1.0.0",
  "description": "A sample Universal React application",
  "main": "server.js",
  "scripts": {
    "test:lint": "eslint src/",
    "test": "npm run test:lint",
    "start": "npm run build && node src/server-es5.js",
    "start:universal": "export UNIVERSAL=true && npm run start",
    "start:dev": "export NODE_ENV=development && nodemon src/server.js",
    "start:dev:universal": "export NODE_ENV=development && export UNIVERSAL=true && npm run start:dev",
    "build": "NODE_ENV=production webpack -p --profile --display-modules --optimize-minimize",
    "build:dev": "webpack -d",
    "build:dev:watch": "webpack -d --watch",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Malik Aliyev",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.2",
    "es6-promise": "^4.2.2",
    "express": "5.0.0-alpha.5",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "snyk": "^1.41.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.8",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "nib": "^1.1.2",
    "nodemon": "^1.11.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "snyk": true
}
